{
  "name": "YACHT",
  "version": "0.0.2",
  "description": "YACHT Another Custom Hugo Theme",
  "private": true,
  "scripts": {
    "build": "npm-run-all webpack hugo-build",
    "dev": "npm-run-all webpack --parallel webpack-watch hugo-server",
    "hugo-build": "hugo --cleanDestinationDir",
    "hugo-server": "hugo server --disableFastRender -s exampleSite -t ../..",
    "webpack": "webpack",
    "webpack-watch": "webpack --watch"
  },
  "dependencies": { },
  "devDependencies": {
    "@catppuccin/palette": "github:catppuccin/palette",
    "bulma": "^>0.9.4",
    "webpack": "^>5.88.2",
    "webpack-cli": "^>5.1.4"
  }
}